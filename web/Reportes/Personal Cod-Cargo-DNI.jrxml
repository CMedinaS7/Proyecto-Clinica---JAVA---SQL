		<![CDATA[SELECT     dbo.Personal.IDPersonal,dbo.Personal.DNI, dbo.Personal.Apellidos + ' ' + dbo.Personal.Nombre as Personal,
 dbo.Personal.FechaNacimiento, dbo.Personal.Telefono,
                      dbo.Personal.Direccion, dbo.Personal.FechaIngreso,turno
FROM         dbo.Personal INNER JOIN
                      dbo.Cargo ON dbo.Personal.IDCargo = dbo.Cargo.IDCargo INNER JOIN
                      dbo.Horario ON dbo.Personal.IDHorario = dbo.Horario.IDHorario where dbo.Personal.IDPersonal=$P{cod} or dbo.Personal.DNI=$P{cod} or dbo.Cargo.NombreCargo=$P{cod}]]>
